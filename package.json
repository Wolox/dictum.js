{
  "name": "dictum.js",
  "version": "1.0.4",
  "description": "Create automatic documentation of your NodeJS API endpoints",
  "engines": {
    "node": "*"
  },
  "main": "lib/index.js",
  "scripts": {
    "coverage": "./node_modules/.bin/istanbul cover ./node_modules/.bin/_mocha",
    "coveralls": "cat ./coverage/lcov.info | ./node_modules/.bin/coveralls",
    "test": "./node_modules/.bin/_mocha test/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wolox/dictum.js.git"
  },
  "keywords": [
    "nodejs",
    "documentation",
    "api",
    "endpoints",
    "tests"
  ],
  "author": "Michel Agopian <michel.agopian@wolox.com.ar>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/wolox/dictum.js/issues"
  },
  "homepage": "https://github.com/wolox/dictum.js#readme",
  "devDependencies": {
    "chai": "^3.5.0",
    "chai-spies": "^0.7.1",
    "coveralls": "^2.13.1",
    "istanbul": "^0.4.5",
    "mocha": "^3.4.1"
  },
  "dependencies": {
    "rimraf": "^2.6.1"
  }
}
